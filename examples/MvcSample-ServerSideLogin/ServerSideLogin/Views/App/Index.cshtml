@using System.Security.Claims;

@{
    ViewBag.Title = "Index";
}

<h2>This page is secured.</h2>

<table>
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Claim claim in (User.Identity as ClaimsIdentity).Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

